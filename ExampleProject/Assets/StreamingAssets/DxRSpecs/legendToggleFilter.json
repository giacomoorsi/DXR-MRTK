{
    "data" : {
      "url" : "cars.json"
    },
    "mark" : "sphere",
    "encoding": {
      "color": {
        "field": "Origin",
        "type": "nominal",
        "legend": {
            "filter": true
          }
      },
      "x": {
        "field": "Horsepower",
        "type": "quantitative",
        "scale": {
          "domain": [ 0, 250 ]
        }
      },
      "y": {
        "field": "Miles_per_Gallon",
        "type": "quantitative",
        "scale": {
          "domain": [ 0, 50 ]
        }
      }
    },
    "interaction": [
      {
        "type": "toggleFilter",
        "field": "Cylinders"
      },
      {
        "type": "thresholdFilter",
        "field": "Displacement"
      }
    ]
  }